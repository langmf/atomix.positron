{
	"Dim Statement": {
		"prefix": "Dim",
		"body": [
			"Dim ${1:Name} As ${2:Byte}\n$0"
		],
		"description": "dim...",
	},

	"Symbol Statement": {
		"prefix": "Symbol",
		"body": [
			"Symbol ${1:Name} = ${2:true}\n$0"
		],
		"description": "symbol...",
	},

	"$define Statement": {
		"prefix": "$define",
		"body": [
			"\\$define ${1:Name}() ${2:true}\n$0"
		],
		"description": "$define...",
	},

	"CData Statement": {
		"prefix": "CData",
		"body": [
			"${1:Label}:	CData ${0:1,2,3}"
		],
		"description": "CData...",
	},
	
	"New Proc": {
		"prefix": "Proc",
		"body": [
			"Proc ${1:Name}($2)",
				"\t$0",
			"EndProc"
		],
		"description": "new proc...",
	},
	
	"New Sub": {
		"prefix": "Sub",
		"body": [
			"Sub ${1:Name}()",
				"\t$0",
			"EndSub"
		],
		"description": "new sub...",
	},

	"If Statement": {
		"prefix": "If ",
		"body": [
			"If ${1:True} Then $0"
		],
		"description": "Single-line If statement",
	},
	
	"If Block": {
		"prefix": "If ",
		"body": [
			"If ${1:True} Then",
				"\t$0",
			"EndIf"
		],
		"description": "Multi-line If block",
	},

	"If Else Block": {
		"prefix": "If Else",
		"body": [
			"If ${1:True} Then",
				"\t$0${2:",
			"Else",
				"\t}",
			"EndIf"
		],
		"description": "Multi-line If Else block",
	},

	"ElseIf Statement": {
		"prefix": "ElseIf",
		"body": [
			"ElseIf ${1:True} Then",
				"\t$0"
		],
		"description": "elseif...",
	},

	"Else Statement": {
		"prefix": "Else",
		"body": [
			"Else",
				"\t${0:True}"
		],
		"description": "else...",
	},
	
	"Select": {
		"prefix": "Select",
		"body": [
			"Select ${1:statement}",
				"\tCase ${2}",
				"\t\t${3}",
				"\tCase Else",
				"\t\t${4}",
			"EndSelect"
		],
		"description": "select...",
	},

	"For Next": {
		"prefix": "For Next",
		"body": [
			"For ${1:i} = 0 To ${2:endcount}",
				"\t$0",
			"Next"
		],
		"description": "for... next",
	},
	
	"For DownTo": {
		"prefix": "For DownTo",
		"body": [
			"For ${1:i} = 0 DownTo ${2:endcount}",
				"\t$0",
			"Next"
		],
		"description": "for... DownTo",
	},
	
	"For UpTo": {
		"prefix": "For UpTo",
		"body": [
			"For ${1:i} = 0 UpTo ${2:endcount}",
				"\t$0",
			"Next"
		],
		"description": "for... UpTo",
	},

	"For Step": {
		"prefix": "For Step",
		"body": [
			"For ${1:i} = 0 To ${2:endcount} Step ${3:stepval}",
				"\t$0",
			"Next"
		],
		"description": "for... step",
	},
	
	"Do Loop Until": {
		"prefix": "Do Until",
		"body": [
			"Do",
				"\t$0",
			"Loop Until ${1:statement}"
		],
		"description": "do loop until",
	},

	"Do Loop While": {
		"prefix": "Do While",
		"body": [
			"Do",
				"\t$0",
			"Loop While ${1:statement}"
		],
		"description": "do loop while",
	},

	"Repeat Until": {
		"prefix": "Repeat",
		"body": [
			"Repeat",
				"\t$0",
			"Until ${1:statement}"
		],
		"description": "repeat until",
	},
	
	"While Wend": {
		"prefix": "While",
		"body": [
			"While ${1:statement}",
				"\t$0",
			"Wend"
		],
		"description": "while wend",
	}
}
